			$("#agreeclose").hide();
			$("#mandatorymsg").hide();
			$("#agreecontinue").show();
			$("#viewdownloadbtn").show();
			
		 });
		 
		 $("#notagree").click(function() {
			$("#viewdownloadbtn").hide();			
			$("#agreecontinue").hide();
			$("#mandatorymsg").show();
			$("#agreeclose").show();
			
		 });	

		$('#nrenroModal').on('hidden.bs.modal', function () {
			$(this)
				.find("input[type=radio]")
				.prop("checked", "")
				.end();
				
				$("#viewdownloadbtn").hide();
				$("#mandatorymsg").hide();
				$("#agreecontinue").hide();
				$("#agreeclose").show();
		});		 
		//NRE/NRO Modal
         
         }); 
         
      </script>
